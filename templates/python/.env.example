# App
ENV=dev
APP_NAME=clinical-service
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000
UVICORN_APP=app.main:app

# Security (dev values only)
JWT_SECRET=dev-only-change-me
JWT_ALG=HS256
TOKEN_TTL_MIN=30
HMAC_SECRET=dev-only-change-me

# Postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=app_dev
POSTGRES_USER=app
POSTGRES_PASSWORD=app_dev_password
DATABASE_URL=postgresql+psycopg://app:app_dev_password@postgres:5432/app_dev

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_URL=redis://redis:6379/0
